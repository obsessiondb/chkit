{
  "name": "chkit",
  "version": "0.1.0-beta.6",
  "homepage": "https://chkit.obsessiondb.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/obsessiondb/chkit.git",
    "directory": "packages/cli"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "chkit": "./dist/bin/chkit.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "bun run src/bin/chkit.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "biome lint src",
    "test": "bun test src --concurrent --timeout 15000",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@chkit/clickhouse": "workspace:*",
    "@chkit/codegen": "workspace:*",
    "@chkit/core": "workspace:*",
    "@stricli/core": "^1.2.5",
    "fast-glob": "^3.3.2"
  }
}
